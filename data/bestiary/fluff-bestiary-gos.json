		},
		{
			"name": "壳鲨",
			"ENG_name": "Shell Shark",
			"source": "GoS",
			"entries": [
				{
					"type": "entries",
					"entries": [
						{
							"type": "entries",
							"entries": [
								{
									"type": "entries",
									"entries": [
