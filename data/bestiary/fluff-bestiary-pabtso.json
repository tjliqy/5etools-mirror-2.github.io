		{
			"name": "蛰伏伪怪突变体",
			"ENG_name": "Cloaker Mutate",
			"source": "PaBTSO",
			"_copy": {
				"name": "变异体",
				"ENG_name": "Mutates",
				"source": "PaBTSO",
				"_mod": {
