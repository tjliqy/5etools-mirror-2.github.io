		{
			"name": "野性灰烬灵",
			"ENG_name": "Feral Ashenwight",
			"source": "PaBTSO",
			"_copy": {
				"name": "灰烬守卫",
				"ENG_name": "Ashenwights",
				"source": "PaBTSO",
